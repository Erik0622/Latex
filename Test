\documentclass{article}
\usepackage{graphicx}
\usepackage{luacode}

\begin{document}

\begin{luacode}
local http = require("socket.http")
local body, code = http.request("https://upload.wikimedia.org/wikipedia/commons/thumb/3/3b/Parabola.svg/400px-Parabola.svg.png")
local file = io.open("image.png", "wb")
file:write(body)
file:close()
\end{luacode}

\begin{figure}[h]
  \centering
  \includegraphics[width=0.7\linewidth]{image.png}
\end{figure}

\end{document}
