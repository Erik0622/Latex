\documentclass{article}

\usepackage{graphicx}
\usepackage{luacode}

\begin{document}

\begin{luacode}
local http = require("socket.http")
local body, code = http.request("https://upload.wikimedia.org/wikipedia/commons/3/3b/Parabola.svg")

local file = io.open("image.svg", "wb")
file:write(body)
file:close()
\end{luacode}

\includegraphics[width=0.7\linewidth]{image.svg}

\end{document}
